{
  "version": 2,
  "builds": [
    {
      "src": "Front13/package.json",
      "use": "@vercel/react"
    },
    {
      "src": "Back13/index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/$1" 
    },
    {
      "src": "/(.*)",
      "dest": "/Front13/$1" 
    }
  ]
}
